{
  "name": "nest-new",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "cross-env ENV=dev nest start --watch",
    "dev:debug": "cross-env ENV=dev nest start --watch --debug ${DEBUG_HOST}:${DEBUG_PORT}",
    "entrypoint": "npm run dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "db:migrate": "rimraf .cache/.migrations && pnpm drizzle-kit generate:pg --config=./drizzle.config.ts && pnpm tsx ./src/database/migrate.ts && drizzle-kit push:pg",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@binochoi/eslint-config-typescript": "^0.0.25",
    "@binochoi/nestjs-mapped-types": "^0.0.5",
    "@fastify/compress": "^7.0.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/static": "^7.0.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.2.1",
    "@nestjs/core": "^10.2.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.2.1",
    "@nestjs/swagger": "^7.1.17",
    "@swc/cli": "^0.3.0",
    "@swc/core": "^1.4.8",
    "@types/bcryptjs": "^2.4.4",
    "@types/pg": "^8.10.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "drizzle-kit": "=0.20.14",
    "drizzle-orm": "^0.29.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "fastify": "^4.24.3",
    "lucia": "^3.1.1",
    "nanoid": "=3.3.7",
