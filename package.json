{
  "name": "nest-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "app": "pnpm --ignore-workspace-root-check",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env ENV=prod node dist/main",
    "dev": "cross-env ENV=dev nest start --debug ${DEBUG_HOST}:${DEBUG_PORT} --watch",
    "dev:entrypoint": "pnpm db:migrate & pnpm dev & pnpm db:studio",
    "db:migrate": "rimraf drizzle && pnpm drizzle-kit generate:pg --config=./drizzle.config.ts && pnpm tsx ./src/database/migrate.ts",
    "db:studio": "pnpm drizzle-kit studio --port 3300 --verbose",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@binochoi/eslint-config-typescript": "latest",
    "@fastify/compress": "^6.4.0",
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "@nestjs/cli": "^10.1.16",
    "@nestjs/common": "^10.2.1",
    "@nestjs/core": "^10.2.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-fastify": "^10.2.1",
    "@types/pg": "^8.10.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "drizzle-kit": "^0.20.0",
    "drizzle-orm": "^0.29.0",
    "fastify": "^4.24.3",
    "nest-typed-config": "^2.7.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.2.0",
    "dotenv": "^16.3.1",
    "rxjs": "^7.8.1",
    "ts-loader": "^9.4.4",
    "@types/bcryptjs": "^2.4.4"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.7",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.48.0",
    "jest": "29.7.0",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
